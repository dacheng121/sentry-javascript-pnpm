{
  "name": "@monitor-web/browser",
  "version": "1.0.0",
  "description": "",
  "main": "build/npm/cjs/index.js",
  "module": "build/npm/esm/index.js",
  "types": "build/npm/types/index.d.ts",
  "scripts": {
    "build": "run-p build:transpile build:bundle build:types",
    "build:dev": "pnpm build",
    "build:bundle": "run-p build:bundle:es5 build:bundle:es6",
    "build:bundle:es5": "JS_VERSION=es5 rollup -c rollup.bundle.config.js",
    "build:bundle:es6": "JS_VERSION=es6 rollup -c rollup.bundle.config.js",
    "build:transpile": "rollup -c rollup.npm.config.js",
    "build:types": "run-s build:types:core build:types:downlevel",
    "build:types:core": "tsc -p tsconfig.types.json",
    "build:types:downlevel": "pnpm downlevel-dts build/npm/types build/npm/types-ts3.8 --to ts3.8",
    "clean": "rimraf build coverage .rpt2_cache sentry-browser-*.tgz"
  },
  "dependencies": {
    "@monitor-web/tracing": "1.0.0",
    "@monitor-web/core": "1.0.0",
    "@monitor-web/types": "1.0.0",
    "@monitor-web/utils": "1.0.0",
    "tslib": "^2.4.1 || ^1.9.3"
  },
  "devDependencies": {
    "@monitor-web/integration-shims": "1.0.0",
    "@types/md5": "2.1.33",
    "btoa": "1.2.1",
    "chai": "4.3.4",
    "chokidar": "3.5.3",
    "fake-indexeddb": "4.0.1",
    "karma": "6.3.16",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-firefox-launcher": "2.1.2",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.5",
    "karma-rollup-preprocessor": "7.0.7",
    "karma-sinon": "1.0.5",
    "karma-typescript": "4.1.1",
    "karma-typescript-es6-transform": "4.1.1",
    "karma-webkit-launcher": "1.0.2",
    "node-fetch": "2.6.7",
    "sinon": "7.5.0",
    "webpack": "4.47.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
